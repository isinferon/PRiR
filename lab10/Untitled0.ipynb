        "def prostokatow(function, a, b, i):\r\n",
        "    dx = (b - a) / i\r\n",
        "    integr = 0\r\n",
        "    for x in range(i):\r\n",
        "        x = x * dx + a\r\n",
        "        integr += dx * eval(function)\r\n",
        "    return integr\r\n",
        "integral = prostokatow('x**2', 0.0, 1.0, 100)\r\n",
        "integral\r\n",
        "\r\n"
      ],
